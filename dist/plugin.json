      "hash": "ab1754a65ae0390bfdb6415e2d79f7f60d4bd4f5",
      "number": 1630
    }
  },
  "routes": [
    {
      "path": "init",
      "method": "*",
      "url": "{{ .JsonData.apiHost }}/api/v1/register/init",
      "reqRole": "Editor"
    },
    {
      "path": "install",
      "method": "*",
      "url": "{{ .JsonData.apiHost }}/api/v1/register/install",
      "reqRole": "Editor",
      "headers": [
        {
          "name": "Authorization",
          "content": "Bearer {{ .SecureJsonData.publisherToken }}"
        }
      ]
    }
  ],
  "includes": [
    {
      "type": "page",
      "name": "Home",
      "path": "/a/grafana-synthetic-monitoring-app/?page=redirect&dashboard=summary",
      "addToNav": true,
      "defaultNav": true
    },
    {
      "type": "page",
      "name": "Checks",
      "path": "/a/grafana-synthetic-monitoring-app/?page=checks",
      "addToNav": true
    },
    {
      "type": "page",
      "name": "Probes",
      "path": "/a/grafana-synthetic-monitoring-app/?page=probes",
      "addToNav": true
    },
    {
      "type": "page",
      "name": "Alerts",
      "path": "/a/grafana-synthetic-monitoring-app/?page=alerts",
      "addToNav": true
    }
  ],
  "dependencies": {
    "grafanaVersion": "7.2.x",
    "plugins": [
      {
        "type": "panel",
        "name": "Worldmap Panel",
        "id": "grafana-worldmap-panel",
        "version": "^0.3.2"
      }
    ]
  }
