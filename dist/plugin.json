{
  "type": "app",
  "name": "Synthetic Monitoring",
  "id": "grafana-synthetic-monitoring-app",
  "info": {
    "author": {
      "name": "Grafana Labs",
      "url": "https://grafana.com"
    },
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "screenshots": [
      {
        "name": "Summary Dashboard",
        "path": "img/screenshot-dash-summary.png"
      },
      {
        "name": "HTTP Dashboard",
        "path": "img/screenshot-dash-http.png"
      },
      {
        "name": "Check List",
        "path": "img/screenshot-check-list.png"
      }
    ],
    "version": "0.8.2",
    "updated": "2021-03-02",
    "build": {
      "time": 1614720452051,
      "repo": "git@github.com:grafana/synthetic-monitoring-app.git",
