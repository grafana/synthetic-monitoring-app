{
  "name": "synthetic-monitoring-app",
  "version": "1.10.0",
  "description": "Grafana Synthetic Monitoring App",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "yarn cypress install && yarn grafana-e2e run",
    "e2e:update": "yarn cypress install && yarn grafana-e2e run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:dashboards": "./scripts/lint-dashboards.sh",
    "lint:fix": "yarn lint --fix",
    "prepare": "husky install",
    "release": "release-it",
    "release:ci": "release-it --ci",
    "release:test": "release-it --dry-run --ci --no-git.requireCleanWorkingDir",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "test": "jest --watch --onlyChanged",
    "test:accessibility": "pa11y --runner axe --reporter json http://localhost:3000/a/grafana-synthetic-monitoring-app/?page=checks > pa11y-report.json",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "test:watch": "jest --watch --onlyChanged",
    "typecheck": "tsc --noEmit"
  },
  "author": "Grafana Labs",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@grafana/e2e": "9.3.8",
    "@grafana/e2e-selectors": "9.3.8",
    "@grafana/eslint-config": "^5.1.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@release-it/conventional-changelog": "^3.0.0",
    "@swc/core": "^1.2.144",
    "@swc/helpers": "^0.4.12",
    "@swc/jest": "^0.2.23",
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "13.5.0",
    "@types/d3": "^6.7.0",
    "@types/d3-hexbin": "^0.2.3",
    "@types/eslint": "^7.2.4",
    "@types/glob": "^8.0.0",
    "@types/ip-address": "^7.0.0",
    "@types/is-base64": "^1.1.0",
    "@types/jest": "^29.2.2",
    "@types/lodash": "^4.14.188",
    "@types/node": "^18.11.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-simple-maps": "^1.0.7",
    "@types/valid-url": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint": "8.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.10.1",
    "eslint-webpack-plugin": "^3.1.1",
    "fork-ts-checker-webpack-plugin": "^7.2.0",
    "glob": "^8.0.3",
    "husky": "^6.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "pa11y": "^6.1.1",
    "prettier": "^2.5.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "release-it": "^14.8.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
