{
  "name": "synthetic-monitoring-app",
  "version": "1.14.12",
  "description": "Grafana Synthetic Monitoring App",
  "scripts": {
    "i18n:betterer-update": "make -C ./.i18n-tooling i18n-betterer-update",
    "i18n:betterer-test": "make -C ./.i18n-tooling i18n-betterer-test",
    "i18n:extract": "make -C ./.i18n-tooling i18n-extract",
    "i18n:upload": "make -C ./.i18n-tooling i18n-upload",
    "i18n:download": "make -C ./.i18n-tooling i18n-download",
    "i18n:script": "bash .i18n-tooling/script.sh",
    "build": "webpack -c webpack.config.ts --env production",
    "dev": "webpack -w -c webpack.config.ts --env development",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:dashboards": "./scripts/lint-dashboards.sh",
    "lint:fix": "yarn run lint --fix",
    "prepare": "husky install",
    "release": "release-it",
    "release:ci": "release-it --ci",
    "release:test": "release-it --dry-run --ci --no-git.requireCleanWorkingDir",
    "server": "docker-compose rm -f && docker-compose up --build",
    "server-dev": "GRAFANA_IMAGE='grafana-dev' GRAFANA_VERSION='10.2.0-131258-ubuntu' yarn server",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "test": "jest --all",
    "test:changed": "jest --watch --onlyChanged",
    "typecheck": "tsc --noEmit"
  },
  "author": "Grafana Labs",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@crowdin/cli": "^4.1.1",
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/eslint-plugin": "link:./.i18n-tooling/packages/grafana-eslint-rules",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@release-it/conventional-changelog": "^3.0.0",
    "@swc/core": "^1.3.62",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "0.2.29",
    "@tanstack/eslint-plugin-query": "5.8.4",
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "15.0.7",
    "@testing-library/user-event": "14.5.2",
    "@types/eslint": "^7.2.4",
    "@types/glob": "^8.0.0",
    "@types/ip-address": "^7.0.0",
