{
  "name": "synthetic-monitoring-app",
  "version": "1.9.24",
  "description": "Grafana Synthetic Monitoring App",
  "scripts": {
    "lint": "eslint --cache --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:dashboards": "./scripts/lint-dashboards.sh",
    "build": "grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "test:accessibility": "pa11y --runner axe --reporter json http://localhost:3000/a/grafana-synthetic-monitoring-app/?page=checks > pa11y-report.json",
    "sign": "grafana-toolkit plugin:sign",
    "ci-build:finish": "grafana-toolkit plugin:ci-build --finish",
    "ci-package": "grafana-toolkit plugin:ci-package",
    "ci-report": "grafana-toolkit plugin:ci-report",
    "github-publish": "grafana-toolkit plugin:github-publish",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "release": "release-it",
    "release:ci": "release-it --ci",
    "release:test": "release-it --dry-run --ci --no-git.requireCleanWorkingDir",
    "prepare": "husky install"
  },
  "author": "Grafana Labs",
  "license": "Apache-2.0",
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@grafana/data": "9.1.1",
    "@grafana/eslint-config": "^5.0.0",
    "@grafana/runtime": "9.1.1",
    "@grafana/toolkit": "9.1.1",
    "@grafana/ui": "9.1.1",
    "@release-it/conventional-changelog": "^3.0.0",
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "13.5.0",
    "@types/d3": "^6.7.0",
    "@types/d3-hexbin": "^0.2.3",
    "@types/eslint": "^7.2.4",
    "@types/ip-address": "^7.0.0",
    "@types/is-base64": "^1.1.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "latest",
    "@types/node": "^18.7.23",
    "@types/react-router-dom": "5",
    "@types/react-simple-maps": "^1.0.7",
    "@types/valid-url": "^1.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^4.10.1",
    "husky": "^6.0.0",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "pa11y": "^6.1.1",
    "release-it": "^14.8.0",
    "tslib": "^2.3.1"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "d3": "^6.7.0",
    "d3-hexbin": "^0.2.2",
    "ip-address": "^7.1.0",
    "is-base64": "^1.1.0",
    "punycode": "^2.1.1",
    "react-async-hook": "^3.6.1",
    "react-hook-form": "7.2.3",
    "react-popper": "^2.2.5",
    "react-simple-maps": "^2.3.0",
    "valid-url": "^1.0.9",
    "yaml": "^2.1.1"
  }
}
