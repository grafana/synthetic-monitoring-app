{
  "name": "synthetic-monitoring-app",
  "version": "1.12.3",
  "description": "Grafana Synthetic Monitoring App",
  "scripts": {
    "build": "webpack -c webpack.config.ts --env production",
    "dev": "webpack -w -c webpack.config.ts --env development",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:dashboards": "./scripts/lint-dashboards.sh",
    "lint:fix": "yarn run lint --fix",
    "prepare": "husky install",
    "release": "release-it",
    "release:ci": "release-it --ci",
    "release:test": "release-it --dry-run --ci --no-git.requireCleanWorkingDir",
    "server": "docker-compose up --build",
    "server-dev": "GRAFANA_IMAGE='grafana-dev' GRAFANA_VERSION='10.2.0-131258-ubuntu' yarn server",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "test": "jest --watch --onlyChanged",
    "test:accessibility": "pa11y --runner axe --reporter json http://localhost:3000/a/grafana-synthetic-monitoring-app/?page=checks > pa11y-report.json",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "test:changed": "jest --watch --onlyChanged",
    "typecheck": "tsc --noEmit"
  },
  "author": "Grafana Labs",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@grafana/e2e": "9.3.8",
    "@grafana/e2e-selectors": "9.3.8",
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@release-it/conventional-changelog": "^3.0.0",
    "@swc/core": "^1.3.62",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@types/eslint": "^7.2.4",
    "@types/glob": "^8.0.0",
    "@types/ip-address": "^7.0.0",
    "@types/is-base64": "^1.1.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.15.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-simple-maps": "^1.0.7",
    "@types/valid-url": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "8.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.10.1",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "har-to-k6": "^0.13.1",
    "husky": "^6.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "pa11y": "^6.1.1",
    "prettier": "^2.8.7",
    "release-it": "^14.8.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "2.5.3",
    "typescript": "4.8.4",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@grafana/data": "10.1.4",
    "@grafana/faro-web-sdk": "1.2.1",
    "@grafana/runtime": "10.1.4",
    "@grafana/scenes": "1.21.0",
    "@grafana/schema": "10.1.4",
    "@grafana/ui": "10.1.4",
    "@popperjs/core": "^2.9.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "ip-address": "^7.1.0",
    "is-base64": "^1.1.0",
    "punycode": "^2.1.1",
    "react": "18.2.0",
    "react-async-hook": "^3.6.1",
    "react-dom": "18.2.0",
    "react-hook-form": "7.2.3",
    "react-popper": "^2.2.5",
    "react-router-dom": "^5.2.0",
    "react-simple-maps": "^2.3.0",
    "valid-url": "^1.0.9",
    "yaml": "^2.2.2"
  },
  "packageManager": "yarn@1.22.19"
}
