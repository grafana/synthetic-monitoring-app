{
  "name": "synthetic-monitoring-app",
  "version": "1.33.2",
  "description": "Grafana Synthetic Monitoring App",
  "scripts": {
    "analytics-events:generate": "node --experimental-strip-types ./scripts/analytics/main.mts",
    "analyze:bundle": "ANALYZE_BUNDLE=true yarn build",
    "build:probe-api-mappings": "yarn ts-node scripts/probe-api-mappings/build-probe-api-mappings.ts",
    "build:generate-terraform-test-config": "npx tsx scripts/terraform-validation/generate-test-configs.ts",
    "build": "webpack -c webpack.config.ts --env production",
    "danger:ci": "yarn danger ci --dangerfile danger/dangerfile.ts",
    "danger:pr": "yarn danger pr --dangerfile danger/dangerfile.ts",
    "dev:msw": "REACT_APP_MSW=true webpack -w -c webpack.config.ts --env development",
    "dev": "webpack -w -c webpack.config.ts --env development",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "lint:dashboards": "./scripts/lint-dashboards.sh",
    "lint:fix": "yarn run lint --fix",
    "lint": "eslint --cache",
    "prepare": "husky install",
    "release:ci": "release-it --ci",
    "release:test": "release-it --dry-run --ci --no-git.requireCleanWorkingDir",
    "release": "release-it",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "test:changed": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4 --bail",
    "test": "jest --all",
    "verify:probe-api-mappings": "yarn ts-node scripts/probe-api-mappings/verify-probe-api-mappings.ts",
    "verify:terraform-test-config": "./scripts/terraform-validation/verify-terraform-test-config.sh",
    "typecheck": "tsc --noEmit"
  },
  "author": "Grafana Labs",
  "license": "Apache-2.0",
